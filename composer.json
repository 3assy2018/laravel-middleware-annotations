{
    "name": "m3assy/laravelannotation",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Mostafa",
            "email": "3assy@diva-lab.com"
        }
    ],
    "keywords": ["Laravel", "LaravelAnnotation"],
    "require": {
        "illuminate/support": "~5|~6",
        "doctrine/annotations": "^1.8",
        "santigarcor/laratrust": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "M3assy\\LaravelAnnotations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "M3assy\\LaravelAnnotations\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "M3assy\\LaravelAnnotations\\LaravelAnnotationServiceProvider"
            ],
            "aliases": {
                "Annotation": "M3assy\\LaravelAnnotations\\Facades\\Annotation"
            }
        }
    }
}
